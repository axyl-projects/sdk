{
  "name": "@tryghost/limit-service",
  "version": "1.2.13",
  "repository": "https://github.com/TryGhost/SDK/tree/main/packages/limit-service",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "exports": "./cjs/index.js",
  "source": "src/index.ts",
  "scripts": {
    "dev": "echo \"Implement me!\"",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' NODE_ENV=testing c8 --all --reporter text --reporter cobertura mocha --require ts-node/register './test/**/*.test.ts'",
    "lint": "eslint . --cache",
    "build": "yarn build:cjs && yarn build:es && yarn build:types",
    "build:cjs": "esbuild src/*.ts --target=es2020 --bundle --external:@tryghost/errors --outdir=cjs --format=cjs --footer:js=\"module.exports = module.exports.default;\"",
    "build:es": "esbuild src/*.ts --target=es2020 --bundle --external:@tryghost/errors --outdir=es --format=esm",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap --outDir types",
    "prepare": "NODE_ENV=production yarn build",
    "pretest": "yarn build",
    "posttest": "yarn lint"
  },
  "files": [
    "LICENSE",
    "README.md",
    "cjs/",
    "es/",
    "src/",
    "types/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/sinon": "^10.0.20",
    "c8": "8.0.1",
    "date-fns": "^2.30.0",
    "esbuild": "^0.19.5",
    "mocha": "10.2.0",
    "should": "13.2.3",
    "sinon": "17.0.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@tryghost/errors": "^1.3.0"
  }
}
